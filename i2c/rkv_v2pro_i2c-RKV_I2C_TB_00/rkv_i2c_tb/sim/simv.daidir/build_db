#!/bin/sh -e
# This file is automatically generated by VCS.  Any changes you make
# to it will be overwritten the next time VCS is run.
echo "Unified Use model is not supported"
exit -1
vcs '-full64' '-sverilog' '-ntb_opts' 'uvm-1.2' '-debug_all' '-l' 'out/log/elab_.log' 'rkv_i2c_tb' "-Xrebuild" "-makedepends=Makefile.Msimdepends"
gmake -f Makefile.Msimdepends
vcs '-full64' '-sverilog' '-ntb_opts' 'uvm-1.2' '-debug_all' '-l' 'out/log/elab_.log' 'rkv_i2c_tb'  -static_dbgen_only -daidir=$1 2>&1
